{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base"
    ],
    "labels": [
        "dependencies"
    ],
    "enabledManagers": [
        "terraform"
    ],
    "hostRules": [
        {
            "matchHost": "https://api.github.com/repos/gruntwork-io",
            "token": "abc123"
        }
    ],
    "packageRules": [
        {
            "managers": [
                "terraform"
            ],
            "additionalBranchPrefix": "{{baseDir}}-",
            "packagePatterns": [
                ".*"
            ]
        }
    ]
}
